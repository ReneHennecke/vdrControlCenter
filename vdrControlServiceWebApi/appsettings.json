{
  "AllowedHosts": "*",
  "Kestrel": {
    "DisableStringReuse": true,
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100,
      "MaxRequestBodySize": 100000000,
      "KeepAliveTimeout": 120,
      "RequestHeadersTimeout": 60
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
